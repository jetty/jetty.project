        <artifactId>jetty-ee10-jspc-maven-plugin</artifactId>
        <version>12.0.7-SNAPSHOT</version>
      </dependency>
      <dependency>
        <groupId>org.eclipse.jetty.ee10</groupId>
        <artifactId>jetty-ee10-maven-plugin</artifactId>
        <version>12.0.7-SNAPSHOT</version>
